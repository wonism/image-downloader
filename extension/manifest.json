{
  "manifest_version": 2,
  "name": "Image downloader",
  "description": "Download images.",
  "version": "1.0.3",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "64": "icon64.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}

